FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      pd;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    outlet 
    {
        type            zeroGradient;
        //type            fixedFluxPressure;
        //value           uniform 0;
    }
    inlet 
    {
        type            zeroGradient;
        //type            fixedFluxPressure;
        //value           uniform 0;
    }
    bottom
    {
        type            zeroGradient;
    }
    seawall
    {
        type            zeroGradient;
    }
    atmosphere 
    {
        type            totalPressure;
        U               U;
        phi             phi;
        rho             none;
        psi             none;
        gamma           1;
        p0              uniform 0;
        value           uniform 0;
    }
}


